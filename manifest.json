{
  "manifest_version": 3,
  "name": "KOSTA Timer",
  "version": "1.0",
  "description": "KOSTA enter/exit timer extension made by HeeJoo",
  "action": {
    "default_popup": "popup.html",
    "default_icon": "./assets/icon1.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["alarms", "notifications", "storage"],
  "icons": {
    "16": "./assets/icon1.png",
    "48": "./assets/icon1.png",
    "128": "./assets/icon1.png"
  }
}
